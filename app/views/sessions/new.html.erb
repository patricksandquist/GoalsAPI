<h1>Sign In</h1>
<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <label for="Username">
    Username
    <input type="text" name="user[username]" value="<%= @user.username %>">
  </label>

  <label for="Password">
    Password
    <input type="password" name="user[password]" value="<%= @user.password %>">
  </label>
  <input type="submit" value="Sign In">
</form>
